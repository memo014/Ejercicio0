package com.me.mygdxgame;
 class B {
	int x;
}
public class Prueba {

	public static void main(String[] args) {
		B mi_objeto=new B();
		mi_objeto.x=10;
		System.out.print(mi_objeto.x);
		
		B mi_otro_obj=new B();
		mi_otro_obj.x=20;
		System.out.print(" ");
		System.out.print(mi_otro_obj.x);
		
		

	}

}
